/**app.wxss**/
page{
	background-color: #eee;
  /*position: relative;*/
}
.userinfo-avatar {
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
}
.mini-avatar{
  width: 100rpx;
  height: 100rpx;
}
.mask{
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  z-index:1;
  background-color:black;
  opacity:0.2;
}
.custom-class{
  z-index:99;
  margin:auto;
  position:absolute;
  left:50%;
  top:50%;
  transform:translate(-50%,-50%);
  background-color:black;
  opacity:0.5;
  border-radius:6rpx;
  height: 120rpx;
  width: 120rpx;
}
.van-loading{
  position:absolute;
  left:50%;
  top:50%;
  transform:translate(-50%,-50%);

}
.goods_pic{
  width: 150rpx;
  height: 150rpx;
  margin: 20rpx 10rpx 10rpx 20rpx;

}
.pic_box{
  display: flex;
  width: 100%;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.pic_box, .txt_box{
  position:relative;
  bottom:50rpx;
}
.item_right{
  display: inline-block;
  width: 80%;
  margin-top: 20rpx;
  margin-left:30rpx;
}
.msg-item{
  display: flex;
  flex-direction: row;
  border-bottom: 2rpx solid #eee;
  margin-top: 10rpx;
}
.nickName{
  color: #2875b8;
  font-size: 30rpx;
}
.nickName text{
  display: block;
  width: 350rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.item_title{
  font-size: 26rpx;
  margin-top: 10rpx;
  width:70%;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.g_type{
  font-size: 22rpx;
  color: #2875b8;
  margin-top: 10rpx;
}
.pub_time{
  font-size: 22rpx;
  color: gray;
}
.footer{
  color: gray;
  font-size: 20rpx;
  text-align: center;
}
.price{
  color: #e76f33;
  font-size: 40rpx;
  float: right;
  right:66rpx;
  position:relative;
  bottom:62rpx;
}
.price .tag{
  font-size: 26rpx;
}
.slide-image{
  width: 750rpx;
}
.empty{
  text-align:center;
  display:block;
  color:gray;
}
.no-likes{
  padding-top: 100rpx;
}
.list_item{
	display: flex;
	flex-direction: row;
	margin-bottom: 20rpx;
	background-color: white;
	padding: 20rpx;
	position: relative;
}
.list_item image{
	width: 200rpx;
	height: 200rpx;
	border-radius: 8rpx;
	margin-right: 20rpx;
}
.g_center{
	position: relative;
}
.goods_msg{
	display: flex;
	flex-direction: column;
	height:100%;
	justify-content:space-between;
}
.g_tit{
	width: 320rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	margin-bottom: 10rpx;
	font-size: 30rpx;
}
.g_desc{
  width: 320rpx;
  color: gray;
	font-size: 24rpx;
	overflow: hidden; 
	text-overflow: ellipsis;
	display: -webkit-box; 
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2; 
}
.like_num{
	font-size: 24rpx;
	color: gray;
}
.g_right{
	position: absolute;
	right: 20rpx;
}
.p_box{
	color: #e76f33;
	margin-bottom: 30rpx;
}
.msg-item .decrip{
	display: flex;
	flex-direction: row;
}
.msg-item .des_txt{
	margin-top: 20rpx;
	font-size: 28rpx;
	margin-left: 10rpx;
}
.msg-item .des_txt .title{
	display: block;
	width: 360rpx;
	overflow: hidden; 
	text-overflow: ellipsis;
	white-space: nowrap;
}
.msg-item .description{
	font-size: 24rpx;
	color: gray;
	display: block;
	margin-top: 10rpx;
	width: 360rpx;
	overflow: hidden; 
	text-overflow: ellipsis;
	display: -webkit-box; 
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3; 
}
.msg-item .t_box{
	padding-bottom: 20rpx;
}
